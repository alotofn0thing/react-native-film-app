{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"target": "esnext",
		"moduleResolution": "node",
		"jsx": "react-native",
		"noEmit": true,
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true
	},
	"exclude": [
		"node_modules",
		"babel.config.js",
		"metro.config.js",
		"jest.config.js"
	]
}
